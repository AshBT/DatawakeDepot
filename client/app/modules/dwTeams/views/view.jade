div.box
    div.box-header
        h3.box-title
            a(href="" ui-sref="^.list" class="btn btn-default")
                i.fa.fa-arrow-left &nbsp;Back to Teams
    div.box-body
        div
            h2 {{team.name}}

            h3 Members
                ul
                    li( ng-repeat="user in team.users") {{user.name}}
            h3 Raw
                pre {{team|json}}