[
  {
    "name": "data-container",
    "Image": "lukewendling\/data-container:1",
    "DockerFilePath": "",
    "Hostname": "data-container"
  },
  {
    "name": "mongo",
    "Image": "jreeme\/mongo:2.6",
    "DockerFilePath": "docker\/mongo\/2.6",
    "Hostname": "mongo",
    "HostConfig": {
      "VolumesFrom": [
        "data-container"
      ],
      "PortBindings": {
        "27017\/tcp": [
          {
            "HostPort": "27017"
          }
        ]
      }
    }
  },
  {
    "name": "datawake",
    "Image": "jreeme/strongloop:10",
    "DockerFilePath": "docker/strong-pm",
    "Hostname": "datawake",
    "ExposedPorts": {
      "3001/tcp": {}
    },
    "HostConfig": {
      "Links": [
        "mongo:mongo"
      ],
      "PortBindings": {
        "3001/tcp": [
          {
            "HostPort": "3000"
          }
        ],
        "8701/tcp": [
          {
            "HostPort": "8702"
          }
        ]
      }
    },
    "ExpressApps": [
      {
        "GitUrl": "https://github.com/Sotera/DatawakeDepot",
        "GitSrcBranchName": "SUG_Updated",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8702",
        "ServiceName": "Datawake"
      }
    ]
  }
]