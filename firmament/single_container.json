[
  {
    "name": "datawake_all",
    "Image": "jreeme/datawake:18-MAY-2016",
    "DockerFilePath": "",
    "Hostname": "datawake_all",
    "ExposedPorts": {
      "27017/tcp": {},
      "3001/tcp": {},
      "3002/tcp": {},
      "3003/tcp": {},
      "8182/tcp": {},
      "8701/tcp": {}
    },
    "HostConfig": {
      "PortBindings": {
        "3001/tcp": [
          {
            "HostPort": "8082"
          }
        ],
        "3002/tcp": [
          {
            "HostPort": "3002"
          }
        ],
        "8182/tcp": [
          {
            "HostPort": "8182"
          }
        ],
        "8701/tcp": [
          {
            "HostPort": "8701"
          }
        ]
      }
    },
    "ExpressApps": [
      {
        "GitUrl": "https://github.com/Sotera/DatawakeDepot",
        "GitSrcBranchName": "MayDemoNoRancor",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8701",
        "ServiceName": "Datawake",
        "Scripts": [
          {
            "RelativeWorkingDir": ".",
            "Command": "bower",
            "Args": [
              "install",
              "--config.interactive=false"
            ]
          },
          {
            "RelativeWorkingDir": ".",
            "Command": "npm",
            "Args": [
              "install",
              "--ignore-scripts"
            ]
          },
          {
            "RelativeWorkingDir": "./browser-plugins/firefox/",
            "Command": "sh",
            "Args": [
              "_runFF.sh"
            ]
          },
          {
            "RelativeWorkingDir": ".",
            "Command": "slc",
            "Args": [
              "ctl",
              "-C",
              "http://localhost:8701",
              "create",
              "Datawake"
            ]
          }
        ]
      },
      {
        "GitUrl": "https://github.com/Sotera/DatawakeSampleExtractor",
        "GitSrcBranchName": "stanford-ner",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8701",
        "ServiceName": "DatawakeStanfordNerExtractor",
        "Scripts": [
          {
            "RelativeWorkingDir": ".",
            "Command": "slc",
            "Args": [
              "ctl",
              "-C",
              "http://localhost:8701",
              "create",
              "DatawakeStanfordNerExtractor"
            ]
          },
          {
            "RelativeWorkingDir": ".",
            "Command": "slc",
            "Args": [
              "ctl",
              "-C",
              "http://localhost:8701",
              "set-size",
              "DatawakeStanfordNerExtractor",
              "1"
            ]
          }
        ]
      },
      {
        "GitUrl": "https://github.com/justinlueders/PageRancor.git",
        "GitSrcBranchName": "master",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8701",
        "ServiceName": "DatawakeRancor",
        "Scripts":[
          {
            "RelativeWorkingDir":".",
            "Command": "bower",
            "Args":[
              "install",
              "--config.interactive=false"
            ]
          },
          {
            "RelativeWorkingDir": ".",
            "Command": "slc",
            "Args": [
              "ctl",
              "-C",
              "http://localhost:8701",
              "create",
              "DatawakeRancor"
            ]
          },
          {
            "RelativeWorkingDir": ".",
            "Command": "slc",
            "Args": [
              "ctl",
              "-C",
              "http://localhost:8701",
              "set-size",
              "DatawakeRancor",
              "1"
            ]
          },
          {
            "RelativeWorkingDir": ".",
            "Command": "slc",
            "Args": [
              "ctl",
              "-C",
              "http://localhost:8701",
              "env-set",
              "DatawakeRancor",
              "PORT=8182"
            ]
          }
        ]
      }
    ]
  }
]
